{
  "name": "http-test",
  "displayName": "http-test",
  "description": "Runs HTTP tests on selected .http files. API testing library",
  "publisher": "iyulab",
  "repository": {
    "type": "git",
    "url": "https://github.com/iyulab-rnd/http-test"
  },
  "icon": "logo_128.png",
  "version": "1.0.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Testing"
  ],
  "keywords": [
    "development",
    "http",
    "test",
    "utilities",
    "api",
    "testing",
    "automatic",
    "assertion",
    "assert"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:http",
    "onCommand:extension.http_test.run",
    "onCommand:extension.http_test.runVerbose"
  ],
  "contributes": {
  "commands": [
    {
      "command": "extension.http_test.run",
      "title": "Run HTTP Test",
      "icon": {
        "light": "assets/light/test.svg",
        "dark": "assets/dark/test.svg"
      }
    },
    {
      "command": "extension.http_test.runVerbose",
      "title": "Run HTTP Test (Verbose)",
      "icon": {
        "light": "assets/light/test-verbose.svg",
        "dark": "assets/dark/test-verbose.svg"
      }
    }
  ],
  "menus": {
    "editor/title": [
      {
        "when": "resourceExtname == .http",
        "command": "extension.http_test.run",
        "group": "navigation@1"
      },
      {
        "when": "resourceExtname == .http",
        "command": "extension.http_test.runVerbose",
        "group": "navigation@2"
      }
    ],
    "editor/context": [
      {
        "when": "resourceExtname == .http",
        "command": "extension.http_test.run",
        "group": "z_http-test@1"
      },
      {
        "when": "resourceExtname == .http",
        "command": "extension.http_test.runVerbose",
        "group": "z_http-test@2"
      }
    ],
    "explorer/context": [
      {
        "when": "resourceExtname == .http",
        "command": "extension.http_test.run",
        "group": "z_http-test@1"
      },
      {
        "when": "resourceExtname == .http",
        "command": "extension.http_test.runVerbose",
        "group": "z_http-test@2"
      }
    ],
    "commandPalette": [
      {
        "command": "extension.http_test.run",
        "when": "resourceExtname == .http"
      },
      {
        "command": "extension.http_test.runVerbose",
        "when": "resourceExtname == .http"
      }
    ]
  }
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "publish": "npx @vscode/vsce publish"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.4",
    "@types/mocha": "^10.0.7",
    "@types/node": "22.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.8.0",
    "typescript": "^5.5.4"
  }
}
